{
    "name": "mmo/faker-images",
    "type": "library",
    "description": "Different images provider for Faker",
    "keywords": [
        "faker",
        "images",
        "picsum",
        "provider"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Marcin Morawski",
            "email": "marcin@morawskim.pl"
        }
    ],
    "require": {
        "php": "^7.1 || ^8.0",
        "fakerphp/faker": "^1.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "rector/rector": "^0.11.53",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "suggest": {
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "Mmo\\Faker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mmo\\Faker\\Test\\": "test/Faker/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "phpcs": "phpcs --standard=PSR2 src -n"
    }
}
